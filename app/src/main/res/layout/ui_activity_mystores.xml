                <TextView
                    android:id="@+id/tv_emailaddress"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@id/tv_star4"
                    android:paddingLeft="@dimen/ten"
                    android:text="@string/my_email"
                    android:textColor="@color/theme_blacker"
                    android:textSize="@dimen/t_eighteen" />

                <EditText
                    android:id="@+id/et_emailaddress"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/fifteen"
                    android:layout_marginRight="@dimen/ten"
                    android:layout_toRightOf="@id/tv_emailaddress"
                    android:background="@color/white"
                    android:gravity="right"
                    android:textSize="@dimen/t_fifteen"
                    android:hint="@string/my_imemail"
                    android:inputType="text" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/fifty"
                android:layout_marginTop="@dimen/two"
                android:background="@color/theme_backgrond"
                android:paddingLeft="@dimen/ten"
                android:paddingRight="@dimen/ten">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/my_status"
                    android:textSize="@dimen/t_twenty"
                    android:textColor="@color/theme_red"
                    android:textStyle="bold"
                    android:layout_centerVertical="true" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/rl_name"
                android:layout_width="match_parent"
                android:layout_height="@dimen/fifty"
                android:layout_marginTop="@dimen/two"
                android:background="@android:color/white"
                android:paddingLeft="@dimen/ten"
                android:paddingRight="@dimen/ten">

                <TextView
                    android:id="@+id/tv_star5"
                    android:layout_width="@dimen/five"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/five"
                    android:text="*"
                    android:textColor="@color/theme_match" />

                <TextView
                    android:id="@+id/tv_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@id/tv_star5"
                    android:paddingLeft="@dimen/ten"
                    android:text="@string/my_relname"
                    android:textColor="@color/theme_blacker"
                    android:textSize="@dimen/t_eighteen" />

                <EditText
                    android:id="@+id/et_name"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/fifteen"
                    android:layout_marginRight="@dimen/ten"
                    android:layout_toRightOf="@id/tv_name"
                    android:background="@color/white"
                    android:gravity="right"
                    android:textSize="@dimen/t_fifteen"
                    android:hint="@string/my_imrelname"
                    android:inputType="text" />
            </RelativeLayout>

            <RelativeLayout
