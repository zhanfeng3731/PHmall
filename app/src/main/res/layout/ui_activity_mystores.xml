                <TextView
                    android:id="@+id/tv_mypicture"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBaseline="@id/tv_star8"
                    android:layout_toRightOf="@id/tv_star8"
                    android:paddingLeft="@dimen/ten"
                    android:text="@string/my_picture"
                    android:textColor="@color/theme_blacker"
                    android:textSize="@dimen/t_eighteen" />

                <Button
                    android:id="@+id/btn_select"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/tv_mypicture"
                    android:layout_marginLeft="@dimen/thirty"
                    android:layout_marginTop="@dimen/five"
                    android:textSize="@dimen/t_fifteen"
                    android:text="@string/my_select" />

                <ImageView
                    android:id="@+id/iv_mypicture"
                    android:layout_width="@dimen/one_hundred"
                    android:layout_height="@dimen/two_hundred"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="@dimen/ten"
                    android:layout_marginTop="@dimen/ten" />
