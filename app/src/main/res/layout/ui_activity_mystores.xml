                android:paddingRight="@dimen/ten">

                <TextView
                    android:id="@+id/tv_star3"
                    android:layout_width="@dimen/five"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/five"
                    android:text="*"
                    android:textColor="@color/theme_match" />

                <TextView
                    android:id="@+id/tv_linkmanphone"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@id/tv_star3"
                    android:paddingLeft="@dimen/ten"
                    android:text="@string/my_phone"
                    android:textColor="@color/theme_blacker"
                    android:textSize="@dimen/t_eighteen" />

                <EditText
