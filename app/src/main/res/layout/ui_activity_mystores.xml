                android:layout_width="match_parent"
                android:layout_height="@dimen/fifty"
                android:layout_marginTop="@dimen/two"
                android:background="@android:color/white"
                android:paddingLeft="@dimen/ten"
                android:paddingRight="@dimen/ten">

                <TextView
                    android:id="@+id/tv_star1"
                    android:layout_width="@dimen/five"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/five"
                    android:text="*"
                    android:textColor="@color/theme_match" />

                <TextView
                    android:id="@+id/tv_address"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@id/tv_star1"
                    android:paddingLeft="@dimen/ten"
                    android:text="@string/my_deaddress"
                    android:textColor="@color/theme_blacker"
                    android:textSize="@dimen/t_eighteen" />

                <EditText
                    android:id="@+id/et_address"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/fifteen"
                    android:layout_marginRight="@dimen/ten"
                    android:layout_toRightOf="@id/tv_address"
                    android:background="@color/white"
                    android:gravity="right"
                    android:textSize="@dimen/t_fifteen"
                    android:hint="@string/my_imdeaddress"
                    android:inputType="text" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/rl_linkmanname"
                android:layout_width="match_parent"
                android:layout_height="@dimen/fifty"
                android:layout_marginTop="@dimen/two"
                android:background="@android:color/white"
                android:paddingLeft="@dimen/ten"
                android:paddingRight="@dimen/ten">

                <TextView
                    android:id="@+id/tv_star2"
                    android:layout_width="@dimen/five"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/five"
                    android:text="*"
                    android:textColor="@color/theme_match" />

                <TextView
                    android:id="@+id/tv_linkmanname"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@id/tv_star2"
                    android:paddingLeft="@dimen/ten"
                    android:text="@string/my_name"
                    android:textColor="@color/theme_blacker"
                    android:textSize="@dimen/t_eighteen" />

                <EditText
                    android:id="@+id/et_linkmanname"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/fifteen"
                    android:layout_marginRight="@dimen/ten"
                    android:layout_toRightOf="@id/tv_linkmanname"
                    android:background="@color/white"
                    android:gravity="right"
                    android:textSize="@dimen/t_fifteen"
                    android:hint="@string/my_imname"
                    android:inputType="text" />
            </RelativeLayout>

            <RelativeLayout
