            android:layout_height="match_parent"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/fifty"
                android:orientation="horizontal"
                android:background="@color/white">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/my_daili"
                    android:textSize="@dimen/t_fifteen"
                    android:textColor="@color/theme_blacker"
                    android:layout_marginLeft="@dimen/ten"
                    android:layout_gravity="center_vertical" />

                <TextView
                    android:id="@+id/tv_agentid"
                    android:layout_width="@dimen/one_hundred_fifty"
                    android:layout_height="@dimen/thirty"
                    android:layout_marginLeft="@dimen/twenty_five"
                    android:background="@color/theme_hint"
                    android:layout_gravity="center_vertical"
                    android:text=""
                    android:textSize="@dimen/t_fifteen"
                    android:gravity="center"
                    android:textColor="@color/theme_red" />
            </LinearLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/fifty"
                android:layout_marginTop="@dimen/two"
                android:background="@color/theme_backgrond"
                android:paddingLeft="@dimen/ten"
                android:paddingRight="@dimen/ten">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/my_storeinfo"
                    android:textSize="@dimen/t_twenty"
                    android:textColor="@color/theme_red"
                    android:textStyle="bold"
                    android:layout_centerVertical="true" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/rl_storename"
                android:layout_width="match_parent"
                android:layout_height="@dimen/fifty"
                android:layout_marginTop="@dimen/two"
                android:background="@android:color/white"
                android:paddingLeft="@dimen/ten"
                android:paddingRight="@dimen/ten">

                <TextView
                    android:id="@+id/tv_star"
                    android:layout_width="@dimen/five"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/five"
                    android:text="*"
                    android:textColor="@color/theme_match" />

                <TextView
                    android:id="@+id/tv_storename"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@id/tv_star"
                    android:paddingLeft="@dimen/ten"
                    android:text="@string/my_storename"
                    android:textColor="@color/theme_blacker"
                    android:textSize="@dimen/t_eighteen" />

                <EditText
                    android:id="@+id/et_realname"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/fifteen"
                    android:layout_marginRight="@dimen/ten"
                    android:layout_toRightOf="@id/tv_storename"
                    android:background="@color/white"
                    android:gravity="right"
                    android:hint="@string/my_imstorename"
                    android:textSize="@dimen/t_fifteen"
                    android:inputType="text" />
            </RelativeLayout>

            <LinearLayout
                android:id="@+id/ll_location"
                android:layout_width="match_parent"
                android:layout_height="@dimen/fifty"
                android:layout_marginTop="@dimen/two"
                android:background="@android:color/white"
                android:paddingLeft="@dimen/ten"
                android:paddingRight="@dimen/ten"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/tv_star00"
                    android:layout_width="@dimen/five"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="@dimen/five"
                    android:text="*"
                    android:textColor="@color/theme_match" />

                <TextView
                    android:id="@+id/tv_managekinds"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:paddingLeft="@dimen/ten"
                    android:text="@string/my_ataddress"
                    android:textStyle="bold"
                    android:textColor="@color/theme_blacker"
                    android:textSize="@dimen/t_eighteen" />

                <Spinner
                    android:id="@+id/spn1"
                    android:layout_width="@dimen/seventy"
                    android:layout_height="@dimen/thirty_five"
                    android:layout_marginLeft="@dimen/ten"
                    android:layout_gravity="center_vertical" />

                <Spinner
                    android:id="@+id/spn2"
                    android:layout_width="@dimen/seventy"
                    android:layout_height="@dimen/thirty_five"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="@dimen/ten" />

                <Spinner
                    android:id="@+id/spn3"
                    android:layout_width="@dimen/seventy"
                    android:layout_height="@dimen/thirty_five"
                    android:layout_marginLeft="@dimen/ten"
                    android:layout_gravity="center_vertical" />
            </LinearLayout>

            <RelativeLayout
