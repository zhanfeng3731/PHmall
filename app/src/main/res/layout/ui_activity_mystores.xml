                <TextView
                    android:id="@+id/tv_idnumber"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@id/tv_star6"
                    android:paddingLeft="@dimen/ten"
                    android:text="@string/my_idnumber"
                    android:textColor="@color/theme_blacker"
                    android:textSize="@dimen/t_eighteen" />

                <EditText
                    android:id="@+id/et_idnumber"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/fifteen"
                    android:layout_marginRight="@dimen/ten"
                    android:layout_toRightOf="@id/tv_idnumber"
                    android:background="@color/white"
                    android:gravity="right"
                    android:hint="@string/my_imidnumber"
                    android:textSize="@dimen/t_fifteen"
                    android:inputType="number" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/rl_myaddress"
                android:layout_width="match_parent"
                android:layout_height="@dimen/fifty"
                android:layout_marginTop="@dimen/two"
                android:background="@android:color/white"
                android:paddingLeft="@dimen/ten"
                android:paddingRight="@dimen/ten">

                <TextView
                    android:id="@+id/tv_star7"
                    android:layout_width="@dimen/five"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/five"
                    android:text="*"
                    android:textColor="@color/theme_match" />

                <TextView
                    android:id="@+id/tv_myaddress"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@id/tv_star7"
                    android:paddingLeft="@dimen/ten"
                    android:text="@string/my_detailaddress"
                    android:textColor="@color/theme_blacker"
                    android:textSize="@dimen/t_eighteen" />

                <EditText
                    android:id="@+id/et_myaddress"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/fifteen"
                    android:layout_marginRight="@dimen/ten"
                    android:layout_toRightOf="@id/tv_myaddress"
                    android:background="@color/white"
                    android:gravity="right"
                    android:hint="@string/my_imdetailaddress"
                    android:textSize="@dimen/t_fifteen"
                    android:inputType="text" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/rl_mypicture"
                android:layout_width="match_parent"
                android:layout_height="@dimen/one_hundred_thirty"
                android:layout_marginTop="@dimen/two"
                android:background="@android:color/white"
                android:paddingLeft="@dimen/ten"
                android:paddingRight="@dimen/ten">

                <TextView
                    android:id="@+id/tv_star8"
                    android:layout_width="@dimen/five"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/five"
                    android:layout_marginTop="@dimen/fifteen"
                    android:text="*"
